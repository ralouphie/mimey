  "license": "MIT",
  "authors": [
    {
      "name": "Ralph Khattar",
      "email": "ralph.khattar@gmail.com"
    }
  ],
  "require": {
    "php": "^5.4|^7.0|^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8 || ^5.7 || ^6.5 || ^9.0",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "autoload": {
    "psr-4": {
